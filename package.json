{
  "name": "myministories",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "clean": "rm -rf node_modules .expo && npm install && expo start -c",
    "doctor": "npx expo-doctor",
    "prebuild": "expo prebuild",
    "clean-native": "rm -rf ios android && expo prebuild --clean",
    "fix-cache": "expo start -c",
    "fix-native": "npm run clean && npm run clean-native",
    "ios:reset": "npm run fix-native && expo run:ios",
    "android:reset": "npm run fix-native && expo run:android",
    "reset-watchman": "watchman watch-del-all || true && rm -rf /usr/local/var/run/watchman/* || true",
    "reset-deriveddata": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "ios:pods": "cd ios && rm -rf Pods Podfile.lock && pod install && cd ..",
    "reset-all": "npm run fix-native && npm run reset-watchman && npm run reset-deriveddata && npm run ios:pods && npm run fix-cache"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "^54.0.25",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image-manipulator": "~14.0.8",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "jpeg-js": "^0.4.4",
    "lucide-react-native": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "@babel/runtime": "7.25.6"
  }
}
